entrypoint = "main.py"
modules = ["python-3.11", "nodejs-20"]
hidden = ["venv", ".pythonlibs"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["python", "main.py"]
deploymentTarget = "gce"
ignorePorts = false

[[ports]]
localPort = 3000
externalPort = 80
exposeLocalhost = true

[[ports]]
localPort = 8000
externalPort = 8000

[env]
PYTHONPATH = "$PYTHONPATH:."
REPLIT_DEV = "1"